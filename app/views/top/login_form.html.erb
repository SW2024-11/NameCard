<%= form_with url: '/top/login', method: :post, local: true do |f| %>
  <body>
    <div class="top">
    <h1>ログイン</h1>
    </div>
    <br>
    <div class="input">
    <%= f.label :my_name, '名前' %>
    <%= f.text_field :my_name, class: "field"%>
    <br>
    <%= f.label :pass, 'パスワード' %>
    <%= f.text_field :pass, class: "field" %>
    </div>
    <br>
    <div class="buttons">
    <%= f.submit 'ログイン' ,class: "registration-button" %>
    </div>
  </body>
<% end %>
