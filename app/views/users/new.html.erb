<%= form_with model: @user, url: users_path do |f| %>
    <h1>新規登録</h1>
    <br>
    <%= f.label :my_company, '会社名' %>
    <%= f.text_field :my_company %>
    <br>
    <%= f.label :my_name, '名前' %>
    <%= f.text_field :my_name %>
    <br>
    <%= f.label :pass, 'パスワード' %>
    <%= f.text_field :pass %>
    <br>
    <%= f.submit '登録' %>
<% end %>