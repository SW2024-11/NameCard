<%= form_with model: @users, url: users_path do |f| %>
　　<h1>編集</h1>
　　<p>
　　<%= f.label :my_company, '会社名' %>
    <%= f.text_field :my_company %>
    </p>
    <p>
    <%= f.label :my_name, '名前' %>
    <%= f.text_field :my_name %>
    </p>
    <p>
    <%= f.label :pass, 'パスワード' %>
    <%= f.text_field :pass %>
    </p>
    <%= f.submit '保存' %>
<% end %>